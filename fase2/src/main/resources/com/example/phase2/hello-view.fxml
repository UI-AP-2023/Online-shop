<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="354.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.phase2.UI.GraphicLoginPageController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Button fx:id="SignupButton" layoutX="220.0" layoutY="257.0" onMouseClicked="#OnSignupClick" text="Sign up" />
   <TextField fx:id="UserNameField" layoutX="125.0" layoutY="60.0" prefHeight="38.0" prefWidth="246.0" promptText="User name" />
   <PasswordField fx:id="PasswordField" layoutX="125.0" layoutY="130.0" prefHeight="38.0" prefWidth="246.0" promptText="Password" />
   <Button fx:id="LoginButton" layoutX="180.0" layoutY="204.0" mnemonicParsing="false" onMouseClicked="#OnLoginButtonClick" prefHeight="26.0" prefWidth="136.0" text="Login" />
   <Label fx:id="invalidParameter" layoutX="125.0" layoutY="306.0" prefHeight="18.0" prefWidth="232.0" />
</AnchorPane>
