<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.phase2.UI.SignupPagerController">
   <TextField fx:id="usernamefield" layoutX="208.0" layoutY="93.0" prefHeight="26.0" prefWidth="184.0"
              promptText="Enter your username"/>
   <TextField fx:id="emailfield" layoutX="208.0" layoutY="133.0" prefHeight="26.0" prefWidth="184.0"
              promptText="Enter your email"/>
   <TextField fx:id="numberfield" layoutX="208.0" layoutY="173.0" prefHeight="26.0" prefWidth="184.0"
              promptText="Enter your phone number"/>
   <PasswordField fx:id="passwordfield" layoutX="208.0" layoutY="215.0" prefHeight="26.0" prefWidth="184.0"
                  promptText="Enter your password"/>
   <Button fx:id="SubmitButton" layoutX="306.0" layoutY="280.0" mnemonicParsing="false" onMouseClicked="#OnSubmit"
           prefHeight="26.0" prefWidth="86.0" text="Submit"/>
   <Button fx:id="BackButton" layoutX="208.0" layoutY="280.0" mnemonicParsing="false" onMouseClicked="#OnBackButton"
           prefHeight="26.0" prefWidth="85.0" text="Back"/>
   <Label fx:id="label" layoutX="135.0" layoutY="49.0" prefHeight="18.0" prefWidth="342.0"/>
</AnchorPane>
